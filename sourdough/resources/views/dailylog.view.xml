        <permission name="read"/>
    </permissions>
    <dependencies>
        <!--    local imports    -->
        <dependency path="dailylog/script.js"/>
        <dependency path="dailylog/styles.css"/>

        <!--    remote imports    -->
        <dependency path="https://cdn.jsdelivr.net/npm/vanillajs-datepicker@1.3.1/dist/css/datepicker.min.css"/>
        <dependency path="https://cdn.jsdelivr.net/npm/vanillajs-datepicker@1.3.1/dist/js/datepicker-full.min.js"/>
    </dependencies>
</view>
